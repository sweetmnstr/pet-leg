    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/db/cliConfig.ts",
    "migration:generate": "yarn typeorm migration:generate ./src/migrations/update-post-table",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "patch": "yarn version --new-version patch",
    "minor": "yarn version --new-version minor",
    "major": "yarn version --new-version major"
  },
